[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/Sebastiano Galeazzo (arch-dev)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/Sebastiano Galeazzo (arch-dev)
	label-Verified = -1..+1 group user/Sebastiano Galeazzo (arch-dev)
	push = group user/Sebastiano Galeazzo (arch-dev)
	read = group user/Sebastiano Galeazzo (arch-dev)
	rebase = group user/Sebastiano Galeazzo (arch-dev)
	removeReviewer = group user/Sebastiano Galeazzo (arch-dev)
	submit = group user/Sebastiano Galeazzo (arch-dev)
[access "refs/heads/*"]
	create = group user/Sebastiano Galeazzo (arch-dev)
	push = +force group user/Sebastiano Galeazzo (arch-dev)
	pushMerge = group user/Sebastiano Galeazzo (arch-dev)
	read = group user/Sebastiano Galeazzo (arch-dev)
[access "refs/tags/*"]
	push = group user/Sebastiano Galeazzo (arch-dev)
	push = +force group user/Sebastiano Galeazzo (arch-dev)
[project]
	state = hidden
